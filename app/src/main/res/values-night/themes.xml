<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ElisApp20" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/quartz</item>
        <item name="colorPrimaryVariant">@color/raisin_black</item>
        <item name="colorOnPrimary">@color/camel</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/quartz</item>
        <item name="colorSecondaryVariant">@color/raisin_black</item>
        <item name="colorOnSecondary">@color/camel</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:colorBackground">@color/lighter_vanilla</item>
    </style>
</resources>